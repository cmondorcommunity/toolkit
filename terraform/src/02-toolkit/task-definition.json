[
  {
    "cpu": 0,
    "essential": true,
    "privileged": true,
    "image": "${image_url}",
    "memory": 2048,
    "name": "${container_name}",
    "portMappings": [
      {
        "containerPort": 8080,
        "protocol": "tcp"
      }
    ],
    "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "${log_group_name}",
            "awslogs-region": "${log_group_region}"
        }
    }
  }
]
